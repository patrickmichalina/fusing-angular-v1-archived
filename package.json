  "scripts": {
    "test": "npm run test.client",
    "test.client": "npm run gen.config && jest --config=test.client.json",
    "test.api": "npm run gen.config && jest --config=test.api.json",
    "test.ci": "npm run test.client -- --ci --updateSnapshot",
    "test.update": "npm run test.client -- --updateSnapshot",
    "test.coverage": "npm run test.client -- --coverage",
    "test.watch": "npm run test.client -- --watch",
    "test.ci.before": "greenkeeper-lockfile-update",
    "test.ci.after": "greenkeeper-lockfile-upload",
    "test.e2e": "npm run gen.config && jest -c e2e-spec.json",
    "test.e2e.ci": "npm run gen.config && npm run start.e2e && concurrently --kill-others --success first \"node dist/server.js --e2e --port 5000\" \"npm run test.e2e\"",
    "test.e2e.ci.aot": "npm run gen.config && npm run start.e2e.aot && concurrently --kill-others --success first \"node dist/server.js --e2e --port 5000\" \"npm run test.e2e\"",
    "test.e2e.dev": "npm run gen.config && npm run start.e2e.dev && concurrently --kill-others --success first \"node dist/server.js --port=5000 --e2e\" \"npm run test.e2e\"",
    "test.e2e.watch": "npm run gen.config && npm run start.e2e.dev && concurrently --kill-others --success first --raw \"node dist/server.js --port=5000 --e2e\" \"jest -c e2e-spec.json -i --watch\" ",
    "start.e2e": "ts-node fuse.ts serve --build-type prod --env-config e2e --ci --port 5000",
    "start.e2e.aot": "ts-node fuse.ts serve --build-type prod --env-config e2e --aot --ci --port 5000",
    "start.e2e.dev": "ts-node fuse.ts serve --build-type dev --env-config dev --ci --e2e",
    "start": "npm run gen.css && concurrently --kill-others --success first \"npm run gen.css.watch\" \"ts-node fuse.ts serve --build-type dev --env-config dev\" || true",
    "start.aot": "rimraf .fusebox && npm run gen.css && ts-node fuse.ts tmp-ng6-upgrade && ngc -p tsconfig-aot.json && ts-node fuse.ts serve --build-type dev --env-config dev --aot",
    "start.deving": "concurrently \"npm run start\" \"npm run test.watch\" \"npm run compodoc\"",
    "start.spa": "ts-node fuse.ts serve --build-type dev --spa",
    "start.prod": "ts-node fuse.ts serve --build-type prod --env-config=prod",
    "start.prod.aot": "rimraf .fusebox && ts-node fuse.ts tmp-ng6-upgrade && ngc -p tsconfig-aot.json && ts-node fuse.ts serve --build-type prod --env-config=prod --aot",
    "start.prod.ci": "ts-node fuse.ts serve --build-type prod --env-config prod --ci --ci-vars",
    "start.prod.ci.aot": "rimraf .fusebox && npm run gen.css && ts-node fuse.ts tmp-ng6-upgrade && ngc -p tsconfig-aot.json && ts-node fuse.ts serve --build-type prod --env-config prod --ci --ci-vars --aot --sw",
    "heroku-postbuild": "npm run start.prod.ci.aot",
    "source.tree.app": "ts-node fuse.ts build --build-only && source-map-explorer dist/js/app.js",
    "source.tree.server": "ts-node fuse.ts build --build-only &&  source-map-explorer dist/server.js",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json -t -s -w -o -r 8081 --theme readthedocs",
    "precommit": "pretty-quick --staged",
    "prepush": "npm run lint && npm test",
    "postmerge": "./tools/scripts/post-merge.sh || true",
    "postinstall": "rimraf .fusebox",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "ts-node fuse.ts lint --build-type prod",
    "bundlesize": "node_modules/.bin/bundlesize",
    "gen.config": "ts-node fuse.ts config",
    "gen.favicon": "ts-node fuse.ts favicon",
    "gen.css": "node_modules/.bin/node-sass src -o src",
    "gen.css.watch": "node_modules/.bin/node-sass src -o src -w",
    "db": "typeorm",
    "setup.mac": "tools/setup/mac.sh || true"
  },
